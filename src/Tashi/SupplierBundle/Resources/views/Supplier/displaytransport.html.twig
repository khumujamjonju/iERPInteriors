<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" >           
    <tr>
        <td class="td-label-title">Purpose List:</td>
    </tr>
    <tr>
        <td>
            <table width="100%" align="center" border="0" cellpadding="0" id="example" cellspacing="0"  class="display"  >  
                <thead>
                    <tr> 
                        <th class="tbl-th-grid-view-row-no-bg">Transporter Name</th>
                        <th class="tbl-th-grid-view-row-no-bg">About US</th>
                        <th class="tbl-th-grid-view-row-no-bg">Action</th>                                               
                    </tr>
                </thead>
                <tbody>
                             {% for jt in allRecord.result%}
                        <tr>
                            <td class="tbl-grid-view-row-no-bg" align="center">{{jt.name | default('')}}</td>
                            <td class="tbl-grid-view-row-no-bg" align="center">{{jt.about | default('')}}</td>
                            <td class="tbl-grid-view-row-no-bg" align="center">
                                <a href="javascript:void();" onclick="retrieveTransport('{{ path('_retrieve_transportor',{ 'transportId': jt.pkid | default('')})}}');">Edit</a> |
                                <a href="javascript:void();" onclick="deleteTransport('{{ path('_delete_transport',{ 'transportId': jt.pkid | default('')})}}','Transportor');">Delete</a>
                            </td>                                               
                        </tr>
                                  {%endfor%}
                    </tbody>
                </table>
            </td>
        </tr>                                 
    </table>