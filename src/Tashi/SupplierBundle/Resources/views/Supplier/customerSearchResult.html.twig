{#<form method="post" post="#"  enctype="multipart/form-data" id="frmCustDetail">#}
    <table width="100%" class="tbl-border"  align="center" border="0" cellpadding="0" cellspacing="0">
    <tr>
        <input id="spanCloseOpenHandlerSearchCustomerResult" value="0" type="hidden">
        <td class="td-label-title" onclick="lmsShowHideAddressResult('SearchCustomerResult');">Supplier List:<span style="float: right;" id="spanIdSearchCustomerResult" class="span_open" >&nbsp;</span></td>
    </tr>
    <tr id="trAddressSearchCustomerResult" >
    <td colspan="6">                  
        <table id="example2" class="display" width="100%" border="0" cellpadding="0" cellspacing="0">
            <thead>
                <tr class="tbl-grid-view-header">
                    <th>Sl. No</th>
                    <th>Supplier Name</th>
                    <th>Registration No</th>
                    <th>Remarks</th> 
                    {#<th>Pan No.</th>#}
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
               {%for r in supplier.result%}                                     
                <tr align="center">
                    <td class="tbl-grid-view-row-no-bg" align="center">{{loop.index}}</td>
                    <td class="tbl-grid-view-row-no-bg" align="center">{{r.supplierFk.companyName}}</td>
                    <td class="tbl-grid-view-row-no-bg" align="center">{{r.supplierFk.registrationNo}}</td>
                    <td class="tbl-grid-view-row-no-bg" align="center">{{r.supplierFk.remarks}}</td>
                    {#<td class="tbl-grid-view-row-no-bg centered-cell">{{result.contact.customerFk.panNo|default('')}}</td>#}

                 <td class="tbl-grid-view-row-no-bg centered-cell" id="saveButton1"  align="center" width="200px">
                    <select class="normal_select" id="prodtype{{r.supplierFk.supplierPk}}" name="prodtype{{r.supplierFk.supplierPk}}" style="width:100px;">
                        <option value="{{ path('_load_sup_detailForm',{'supid': r.supplierFk.supplierPk})}}&edt">Edit</option>
                        <option value="{{ path('_deletesupplier',{'supid':  r.supplierFk.supplierPk})}}&del">Delete</option>
                    </select> 
                   <input class="button" name="commit" type="button" value="Go" onClick="loadEditSupPage('{{r.supplierFk.supplierPk}}',this);"/>
                 </td> 
                </tr>                                           
               {% endfor %}

            </tbody>
            </table>
        </td>   
    </tr>
</table>
    {#</form>#}
      