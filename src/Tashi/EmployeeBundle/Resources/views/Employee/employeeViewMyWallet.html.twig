
<div class='sub-menu'>
    <ul>
        <li class='active'><a href='#' href="javascript:void();" onclick="fnLeftMenuNavigation('{{path('_view_my_wallet')}}', 'view_my_wallet', 'Employee','View My Wallet');"><span>My Wallet</span></a></li>  
       {# <li id="wallet_history"><a href="javascript:void();" onclick="fnSubMenuNavigation('{{path('_emp_wallet_history')}}', 'wallet_history', 'Employee Master', 'Master Setting', 'Wallet History');"><span>Wallet History</span></a></li>  
   #} </ul>
</div>
<div class="newdashboard">  
    <div class="application-form">        
        <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border">                       
            <tr>
                <td class="td-label-title" colspan="4">Balance :</td>
            </tr>
            <tr>
            <td class="td-gray-bg cmnWidth">Current Balance:</td>
            <td class="td-white-bg cmnWidth">
            <input type="text" class="cmnWidth" disabled="true" value="{{balance.balanceAmount | default('')}}"/></td>
            <td class="td-gray-bg cmnWidth"></td>
            <td class="td-white-bg cmnWidth"></td>      
            </tr> 
        </table>
           <br/> 
    <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" >           
            <tr>
                <input id="spanCloseOpenHandlerShowDeposit" value="0" type="hidden">
                <td class="td-label-title" onclick="lmsShowHideAddressResult('ShowDeposit');">My Deposit Details List:
                <span style="float: right;" id="spanIdShowDeposit" class="span_open" >&nbsp;</span></td>
            </tr>
       
            <tr id="trAddressShowDeposit">
                <td>
                    <table width="100%" align="center" border="0" cellpadding="0" id="example" cellspacing="0"  class="display"  >  
                        <thead>
                            <tr>
                               <th class="tbl-th-grid-view-row-no-bg">Source Type</th>
                                <th class="tbl-th-grid-view-row-no-bg">Deposit Date</th>
                                <th class="tbl-th-grid-view-row-no-bg">Amount</th>
                                <th class="tbl-th-grid-view-row-no-bg">Description</th>                                           
                            </tr>
                        </thead>

                        <tbody>                           
                               {%for d in deposit %} 
                                <tr>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{d.sourceType.name |default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{d.recordInsertDate | date('y-m-d') | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{d.amount | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{d.description | default('')}}</td>
                                </tr> 
                               {%endfor%}
                               
                       </tbody>
                        </table>
                    </td>
                </tr>                                 
            </table>        
            
       <br/> 
    <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" >           
            <tr>
                <input id="spanCloseOpenHandlerShowExpense" value="0" type="hidden">
                <td class="td-label-title" onclick="lmsShowHideAddressResult('ShowExpense');">My Expense Details List:
                <span style="float: right;" id="spanIdShowExpense" class="span_open" >&nbsp;</span></td>
            </tr>
            <tr id="trAddressShowExpense">
                <td>
                    <table width="100%" align="center" border="0" cellpadding="0" id="example2" cellspacing="0"  class="display"  >  
                        <thead>
                            <tr>
                                <th class="tbl-th-grid-view-row-no-bg">Expense Type</th>
                                <th class="tbl-th-grid-view-row-no-bg">Project Name</th>
                                <th class="tbl-th-grid-view-row-no-bg">Item Name</th>
                                <th class="tbl-th-grid-view-row-no-bg">Description</th>  
                                <th class="tbl-th-grid-view-row-no-bg">Expense Date</th> 
                                <th class="tbl-th-grid-view-row-no-bg">Amount</th> 
                                <th class="tbl-th-grid-view-row-no-bg">Received Image</th>  
                            </tr>
                        </thead>

                        <tbody>                           
                            {%for e in expense %} 
                                <tr>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{e.expensesType.sourceType |default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{e.projectFk.areaFk.area | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{e.item.productName | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{e.expensesDescription | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{e.expensesDate | date('y-m-d') | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{e.amount | default('')}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center"> 
                                    {%if e.documentFk is null%}
                                        <img src="{{asset("bundles/common/images/noimage.png")}}" alt="image" class="prodlist"/>
                                                                        {%else%}
                                        <img src="{{asset(e.documentFk.path)}}" alt="image" class="prodlist"/>
                                        {%endif%}
                                        </td>
                                </tr> 
                               {%endfor%}  
                       </tbody>
                        </table>
                    </td>
                </tr>                                 
            </table>  
   
</div> 
</div>


 