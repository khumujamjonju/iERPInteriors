

<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" >           
    <tr>
        <td class="td-label-title">Working Type List</td>
    </tr>
    <tr>
        <td>

            <table width="100%" align="center" border="0" cellpadding="0" id="example" cellspacing="0"  class="display"  >  
                <thead>
                    <tr>
                        <th class="tbl-th-grid-view-row-no-bg">Working Type Name</th>
                        <th class="tbl-th-grid-view-row-no-bg">Description</th>
                        <th class="tbl-th-grid-view-row-no-bg">Action</th>                                              
                    </tr>
                </thead>

                <tbody>  
                                {%for wt in allRecord%}

                        <tr>
                            <td class="tbl-grid-view-row-no-bg" align="center">{{wt.workingType}}</td>
                       
                            <td class="tbl-grid-view-row-no-bg" align="center">{{wt.description}}</td>
                             <td class="tbl-grid-view-row-no-bg" align="center">
                     <a href="javascript:void();" onclick="retreiveEmpWorkingType('{{path('_retreive_workingtype',{'workingtypeId': wt.workingTypePk })}}');">Edit </a>
 | <a href="javascript:void();"  style="color:#ff0000;" onclick="deleteEmpWorkingTypeMaster('{{path('_deleteEmpWorkingType',{'workingtypeId': wt.workingTypePk })}}');">Delete </a></td>                                                 
            
                 
                        
                        </tr>

                               {%endfor%}

                    </tbody>
                </table>
            </td>
        </tr>                                 
    </table>
