
<table style="margin-bottom: 0px; width: 100%;"  border="0" name="tabRight"> 
    <tr> 
        <td>
            <ul class="tabs"> 
                <li class="active" rel="tab1" id="subMenuId" >Approve New Customer</li>    <!--  right menu table name   -->        
            </ul>
        </td>
    </tr>
    <tr>
    <td>
      <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border">
       <tr>
          <td class="dash-board-col-mid" >
          <!-- body of the right content start from here -->    
            <form method="post" post="#"  enctype="multipart/form-data" id="frmApproveCustomerform">
                <table  style="width:100%; margin-top: 10px;" class="tbl-border-content"  align="center" border="0" cellpadding="0" cellspacing="0">
                     <tr>
                         <input id="spanCloseOpenHandlerCusApprovalList" value="0" type="hidden">
                         <td class="td-label-title" onclick="lmsShowHideAddressResult('CusApprovalList');">Customer Approval Pending List:<span style="float: right;" id="spanIdCusApprovalList" class="span_open" >&nbsp;</span></td>
                    </tr>
                        <tr id="trAddressCusApprovalList" >
                        <td colspan="6">                  
                        <table id="example" class="display" width="100%" border="0" cellpadding="0" cellspacing="0">
                               <thead>
                               <tr class="tbl-grid-view-header">
                               <th>Customer ID</th>
                               <th>Customer Name</th>
                               <th>Address</th>
                               <th>City</th>
                               <th>MobTXN</th>
                               <th>Action</th>
                               </tr>
                               </thead>
                               <tbody>
                                  
                               {%for res in result%}
                                    <tr align="center">
                                    <td class="tbl-grid-view-row-no-bg centered-cell">{{res.pkid}}</td>
                                    <td class="tbl-grid-view-row-no-bg centered-cell">{{res.customerName}}</td>
                                    <td class="tbl-grid-view-row-no-bg centered-cell">{{res.address}}</td>
                                    <td class="tbl-grid-view-row-no-bg centered-cell">{{res.city}}</td>
                                    <td class="tbl-grid-view-row-no-bg centered-cell">{{res.mobtxnpkid}}</td>
                                    <td colspan="5" class="tbl-grid-view-row-no-bg centered-cell" id="saveButton1"  align="center">
                                    <input class="button" name="commit" type="button" value="Review/Approve" onClick="editUpdateCust('{{path('_load_cus_detailForm',{'pkid':res.mobtxnpkid})}}');" />
                                    </td> 
                                    </tr>
                               {%endfor%}
                                  
                               </tbody>
                              </table>
                         </td>   
                        </tr>
                 </table>
             </form>
          
             <table  style="width:100%;" align="center" border="0" cellpadding="0" cellspacing="0">
            <div id="custDetailArea"> </div>
            </table>
          
      
          </td>
        </tr>
     </table>
    </td> 
  </tr> 
</table>
