                 
<table style="width:100%; margin-top: 5px;"  width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border display">
    <thead>
        <tr class="tbl-grid-view-header">
            <th>Customer ID</th>
            <th>Customer Name</th>
            <th>Contact Person </th>
            <th>Mobile No.</th>
            
        </tr>
    </thead>
    <tbody>
        {% for result in result %}
            {% if result.contact.customerFk.customerId|default('') is not empty %}
            <tr align="center">
                <td class="tbl-grid-view-row-no-bg centered-cell">{{result.contact.customerFk.customerId|default('')}}</td>
                <td class="tbl-grid-view-row-no-bg centered-cell">{{result.contact.customerFk.customerName|default('')}}</td>
                <td class="tbl-grid-view-row-no-bg centered-cell">{{result.contact.personFk.personName|default('')}}</td>
                <td class="tbl-grid-view-row-no-bg centered-cell">{{result.mobileNo.mobileNo|default('')}}</td>
            </tr>
            {% endif %}
       {% endfor %}
            <tr><td style="text-align: center;" colspan="4" class="tbl-grid-view-row-no-bg centered-cell">
                    Do You Want To Proceed? 
                    <button type="button" class="button" onClick="cimSaveNewCustomer('{{ path('_erplms_cim_create_customer_after_confirmation')}}')"> Yes</button>
                    <button type="button" class="button" onclick="cmnEmpty('loadResultOfSearchAction');"> No</button>
                </td></tr>

        </tbody>
</table>

