<br>
<form id="FormMyExpense">
<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" style="margin-top: -20px;">                       
    
     <tr>
                <input id="spanCloseOpenHandlerEditWalletExpenses" value="0" type="hidden">
                <td class="td-label-title" onclick="lmsShowHideAddressResult('EditWalletExpenses');">Edit Expenses Details : <span style="float: right;" id="spanIdEditWalletExpenses" class="span_open" >&nbsp;</span></td>
    </tr>
     
    <tr id="trAddressEditWalletExpenses">
        <td>                 
         <table width="100%" align="center" border="0" cellpadding="0" cellspacing="0">                             
                <tr>
                    <td class="td-gray-bg cmnWidth" >Employee ID:<input type="hidden" name="empid" id="empid" value="{{eid | default('')}}"/></td>
                    <td class="td-white-bg cmnWidth" style="color:green;"><label id="empcode">{{code | default('')}}</label>
                    
                    <input type="hidden" name="empcode" value="{{code| default('')}}"/>
                    <input type="hidden" name="id" value="{{id| default('')}}"/>
                   {# <input type="hidden" name="document" value="{{doc | default('')}}"/>#}
                    <input type="hidden" name="account" value="{{aid | default('')}}"/>
                    </td>
                    <td class="td-gray-bg cmnWidth">Employee Name:</td>
                    <td class="td-white-bg cmnWidth" style="color:green;"><label id="employee">{{name | default('')}}</label>
                    <input type="hidden" name="emppersonk" id="emppk" value="{{eid| default('')}}"/>
                    <input type="hidden" name="accountid" value="{{code | default('')}}"/>
                    </td>
                </tr>
                
                <tr>
                    <td class="td-gray-bg cmnWidth">Expenses Type:<span class="mandatoryField">*</span></td>
                    <td class="td-white-bg cmnWidth">
                    <select class="cmnWidth" id="sourceid" name="sourcename"  onchange="SearchCriteriaforExpenses('{{path('_showlist')}}');">
                    <option  value="">--Select--</option>
                    {%for a in source%}
                     <option value="{{a.pkid| default('')}}">{{a.sourceType| default('')}}</option>
                    {%endfor%}
                    </select>
                    </td>
                    <td class="td-gray-bg cmnWidth"> </td>
                    <td class="td-white-bg">
                        {#<input type="file" disabled="true"  onchange="calculateFileSize(this);" name="fileExpensesImg" id="fileExpensesImg"/>
                    <span id="spanfsize" style="font-size: 11px;"></span>
                        <br/>
                        <span style="color:#666666; font-size: 11px;">(.jpg,.jpeg,.png,.gif,.bmp)- Upto 512Kb</span>
                    #}
                    </td>
                    
                    
                    <tbody id="showdetailssource">
                    <td class="td-gray-bg cmnWidth">Select Project:<span class="mandatoryField">*</span></td>
                    <td class="td-white-bg">
                    <select name="selectlist" id="ddlb_selectlist" class="cmnWidth" onchange="SearchCriteriaforItemsShowing('{{path('_showlistitems')}}');">
                    <option value="">--Select--</option>
                   {%for pro in project %}
                   <option value="{{pro.pkid| default('') }}">{{pro.areaFk.area| default('')}} ({{pro.orderNo| default('')}})</option>
                   {%endfor%}
                   </select>
                   </td></tbody>
                   <tbody id="showitemslist">
                   <td class="td-gray-bg cmnWidth">Select Project Items:<span class="mandatoryField">*</span></td>
                   <td class="td-white-bg">
                   <select name="selectlist1" id="ddlb_itemselectlist" class="cmnWidth">
                   <option value="">--Select--</option>
                   {%for it in item %}
                   {%if it.itemFk is not null %}
                   <option value="{{it.pkid | default('')}}">{{it.itemFk.productName | default('')}}</option>
                   {%endif%}
                   {%endfor%}
                   </select>
                    </td>
                    <td><span>Last Selected ITem: <input type="text" value="" id="Itemshow1"/></span></td>
                    <td><input type="hidden" value="" id="Itemshow"/></td>
                </tbody>
                </tr>
                <tr>
                    <td class="td-gray-bg cmnWidth">Amount:<span class="mandatoryField">*</span></td>
                    <td class="td-white-bg cmnWidth"> <input type="text" name="amount" id="txt_amount" class="cmnWidth"/></td> 
                    <td class="td-gray-bg cmnWidth"></td>
                    <td class="td-white-bg"></td>
                    
                </tr>
                <tr>
                    <td class="td-gray-bg cmnWidth">Description:<span class="mandatoryField">*</span></td>
                     <td class="td-white-bg" colspan="3">
                    <textarea name="description" id="descrp" style="width: 99%;height: 50px;"></textarea>
                    </td>
                   
                    
                </tr>
                <tr>
                    <td class="td-gray-bg cmnWidth"> </td>
                    <td class="td-white-bg cmnWidth"> 
                    <input type="button" class="button" id="btnUpdate" value="Update" 
                    onclick="return UpdateWalletExpensesDetails('{{path('_updateWalletExpenses')}}','FormMyExpense');">
                    <input type="hidden" id="acct_id" name="txt_accntid" value=""/>
                    
                    
                    </td>
                    <td class="td-gray-bg cmnWidth"> </td>
                    <td class="td-white-bg cmnWidth"> </td>
</tr>
                
                
                
               </table>
 
                 
               
               
                
                 
        </td>
    </tr> 
</table> 

</form>



                
        







