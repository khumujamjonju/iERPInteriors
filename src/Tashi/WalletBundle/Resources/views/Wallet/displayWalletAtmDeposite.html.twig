<div id="WalletAtm">
<br/>    
<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border">                       

    <tr>
    <input id="spanCloseOpenHandlerShowWalletDeposit" value="0" type="hidden">
    <td class="td-label-title" onclick="lmsShowHideAddressResult('ShowWalletDeposit');">Employee Wallet ATM Deposit Details : <span style="float: right;" id="spanIdShowWalletDeposit" class="span_open" >&nbsp;</span></td>
</tr>

<tr id="trAddressShowWalletDeposit">
    <td>                 
        <table width="100%" align="center" border="0" cellpadding="0" cellspacing="0">                             
            <tr>
                <td class="td-gray-bg cmnWidth" >Employee ID:<input type="hidden" name="empId" id="empid" value="{{empdetails.empFk.employeePk | default('')}}"/></td>
                <td class="td-white-bg cmnWidth" style="color:green;"><input type="text" disabled="true" id="employeeID" value="{{empdetails.empFk.employeeId | default('')}}">
                    <input type="hidden" name="empcode" value="{{empdetails.employeeId | default('')}}"/>
                    <input type="hidden" name="empid" id="employeepk" value=""/>
                </td>
                <td class="td-gray-bg cmnWidth">Employee Name:</td>
                <td class="td-white-bg cmnWidth" style="color:green;"><input type="text" disabled="true" id="employeeName" value="{{empdetails.empFk.personFk.personName | default('')}}">
                    <input type="hidden" name="emppersonk" id="emppk" value="{{empdetails.empFk.personFk.personPk | default('')}}"/>
                </td>
            </tr>
            <tr>
                <td class="td-gray-bg cmnWidth">Bank Name:<span class="necessary-field">*</span></td>
                <td class="td-white-bg cmnWidth"> <input type="text" name="bankName" id="txt_bank_name" class="cmnWidth"/></td> 
                <td class="td-gray-bg cmnWidth"></td>
                <td class="td-white-bg cmnWidth"></td>
            </tr>
            <tr>
                <td class="td-gray-bg cmnWidth">Account No:<span class="necessary-field">*</span></td>
                <td class="td-white-bg cmnWidth"> <input type="text" name="accountNo" id="txt_account_no" class="cmnWidth"/></td> 
                <td class="td-gray-bg cmnWidth">Card No:<span class="necessary-field">*</span></td>
                <td class="td-white-bg cmnWidth"> <input type="text" name="cardNo" id="txt_card_no" class="cmnWidth"/></td>                    
            </tr>
            <tr>
            <td class="td-gray-bg cmnWidth">Description:</td>
            <td class="td-white-bg" colspan="3">
                    <textarea name="description" id="txt_description" style="width: 98%;"></textarea>
                    </td>
            </tr>

            <tr>
                <td class="td-gray-bg cmnWidth"> </td>
                <td class="td-white-bg cmnWidth"> 
                    <input type="button" class="button" id="btnSave" value="Save" onclick="return SaveWalletAtmDetails('{{path("_saveAtmWallet")}}', this);">
                    <input type="button" class="button" id="btnUpdate" value="Update" onclick="return UpdateWalletAtmDetails('{{path("_updateAtmWallet")}}', this);">
                    <input type="hidden" id="atm_id" name="txt_atmid" value=""/>                   
                </td>
                <td class="td-gray-bg cmnWidth"> </td>
                <td class="td-white-bg cmnWidth"> </td>
            </tr>  
        </table>      
    </td>
</tr>   
</table> </div> 
<br/> 
<div id="displayWalletDetails">
   
  <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" >           
            <tr>
                <td class="td-label-title">Employee Wallet Details List:</td>
            </tr>
            <tr>
                <td>
                    <table width="100%" align="center" border="0" cellpadding="0" id="example" cellspacing="0"  class="display"  >  
                        <thead>
                            <tr>
                                <th class="tbl-th-grid-view-row-no-bg">Sl No</th>
                                <th class="tbl-th-grid-view-row-no-bg">Employee Name</th>
                                <th class="tbl-th-grid-view-row-no-bg">Bank Name</th>
                                <th class="tbl-th-grid-view-row-no-bg">Action</th>                                              
                            </tr>
                        </thead>

                        <tbody>                           
                               
                                {%for w in wallet %} 
                                <tr>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{loop.index}} </td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">
                                    <input type="hidden" name="employeeid" id="txt_eid" value="{{w.empFk.employeePk}}"/>
                                    {{w.empFk.personFk.personName}} </td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">{{w.atmname}}</td>
                                    <td class="tbl-grid-view-row-no-bg" align="center">
                                    <a href="javascript:void();" onclick="RetriveATMDetails('{{ path('_retreiveatm',{'atmid': w.pkid | default('')})}}')">Edit</a> |
                                    <a href="javascript:void();" onclick="deleteWalletATMDetails('{{ path('_deleteWalletAtm',{'atmid': w.pkid | default('')})}}')">Delete</a></td>
                                </tr> 
                               {%endfor%}  
                       </tbody>
                        </table>
                    </td>
                </tr>                                 
            </table>
    
    
</div>