
</br>
<form method="post" name="frmAssignAsset" id="frmAssignAsset">
    <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" id="tblAssetListBycate">           
        <tr>
            <td class="td-label-title">Click On Asset And Assign It</td>
        </tr>
        <tr>
            <td>

                <table width="100%" align="center" border="0" cellpadding="0" id="example2" cellspacing="0"  class="display"  >  
                    <thead>
                        <tr>

                            <th class="tbl-th-grid-view-row-no-bg">Category</th>
                            <th class="tbl-th-grid-view-row-no-bg">Name</th>
                            <th class="tbl-th-grid-view-row-no-bg">Manufacturer</th>
                            <th class="tbl-th-grid-view-row-no-bg">Model No.</th>
                            <th class="tbl-th-grid-view-row-no-bg">Serial No.</th>
                            <th class="tbl-th-grid-view-row-no-bg">Manufacturing Date</th>
                            <th class="tbl-th-grid-view-row-no-bg">Warranty Date</th>
                            <th class="tbl-th-grid-view-row-no-bg">Expiry Date</th>
                            <th class="tbl-th-grid-view-row-no-bg">Status</th>
                            <th class="tbl-th-grid-view-row-no-bg">Click It</th>                                              
                        </tr>
                    </thead>

                    <tbody>  
                              {%for a in projArr%}
                            <tr>
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.assetCategoryMasterFk.assetCategoryName | default('')}}</td>
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.assetName | default('')}}</td>                                                                        
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.manufacturer | default('')}}</td>
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.modelNo | default('')}}</td>
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.serialNo | default('')}}</td>

                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.manufacturingDate|date('d-m-Y')}}</td>
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.warranty|date('d-m-Y')}}</td>
                                <td class="tbl-grid-view-row-no-bg" align="center">{{ a.expiryDate|date('d-m-Y')}}</td>
                                <td class="tbl-grid-view-row-no-bg" style="color:{{a.status.statusColor}}" align="center">
                                                                                 {{a.status.statusName}}
                                </td>
                                <td class="tbl-grid-view-row-no-bg" align="center">
                                    <input type="checkbox"  name="checkAsset" value="{{a.assetRegisterPk}}" >
                                </td>
                            </tr>  
                               {%endfor%}
                        </tbody>

                    </table>
                </td>
            </tr>

        </table>
        <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tbl-border" id="assTbl">

            <tr>
                <td class="td-gray-bg">Start Date:</td>
                <td class="td-white-bg">
                    <input type="date" class="cmnWidth" id="txt_start_date" name="txt_start_date">
                </td>
                <td class="td-gray-bg">End Date:</td>
                <td class="td-white-bg">
                    <input type="date" class="cmnWidth" id="txt_end_date" name="txt_end_date"> 
                </td>                                                          
            </tr>

            <tr>
                <td class="td-separator" colspan="4" style="text-align: center;">
                    <input type="button" class="button blue" id="save" value="Assign" onclick="return saveAssetAssignMaster('{{ path('_saveAssetAssignMaster',{'empId': empId,'employeePk':employeePk})}}', this);"/>
                    <input type="button" class="button blue" id="btn_cancel" value="Cancel" onclick="assetCancel('assTbl', 'AssignedAssAftrSrchId');" style="display: none;"/>
                    <input type="button" class="button blue" id="btn_clear" value="Reset" onclick="formReset();"/>  
                </td>

            </tr>
        </table>
        <input type ="hidden" id="AssignedAssAftrSrchId" name="AssignedAssAftrSrchId" value="" >
    </form>